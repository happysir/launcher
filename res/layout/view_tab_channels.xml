            android:layout_height="@dimen/main_page_short_edge_length"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/image_button_bg"
            android:focusable="true" />

        <com.wefeng.launcher.widget.TabChannelsItemView
            android:id="@+id/tab_channel_button_2"
            android:layout_width="@dimen/main_page_short_edge_length"
