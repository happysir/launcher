            android:focusable="true" />

        <com.wefeng.launcher.widget.TabChannelsItemView
            android:id="@+id/tab_channel_button_11"
            android:layout_width="@dimen/main_page_short_edge_length"
            android:layout_height="@dimen/main_page_short_edge_length"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="@dimen/main_page_shading_offset"
            android:layout_toRightOf="@id/tab_channel_button_9"
            android:background="@drawable/image_button_bg"
            android:focusable="true" />

        <com.wefeng.launcher.widget.TabChannelsItemView
            android:id="@+id/tab_channel_button_12"
            android:layout_width="@dimen/main_page_short_edge_length"
            android:layout_height="@dimen/main_page_short_edge_length"
            android:layout_below="@id/tab_channel_button_11"
            android:layout_marginLeft="@dimen/main_page_shading_offset"
            android:layout_marginTop="@dimen/main_page_shading_offset"
            android:layout_toRightOf="@id/tab_channel_button_10"
            android:background="@drawable/image_button_bg"
            android:focusable="true" />
